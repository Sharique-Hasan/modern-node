{
  "name": "modern-node",
  "version": "0.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2017": "^6.14.0",
    "babel-register": "^6.14.0",
    "babel-watch": "^2.0.2",
    "cross-env": "^2.0.1",
    "xo": "^0.16.0"
  },
  "scripts": {
    "test": "ava",
    "lint": "xo",
    "build": "babel src --out-dir lib"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2017"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "esnext": true
  }
}
